<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <system.webServer>
        <handlers>
        	<add name="iisnode" path="server/app.js" verb="*" modules="iisnode" />
        </handlers>

        <iisnode loggingEnabled="true" debuggingEnabled="true" devErrorsEnabled="true" />

        <rewrite>
            <rules>
                <clear />

                <rule name="node.js">
                    <match url="^(?!(?:bower_|node_))(?:app)/.*(?:\.css)" />
                    <action type="Rewrite" url="server/app.js" />
                </rule>

            </rules>
         </rewrite>

        <staticContent>

        </staticContent>
    </system.webServer>
</configuration>
