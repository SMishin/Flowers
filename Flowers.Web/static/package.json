{
  "name": "flowers",
  "version": "1.0.0",
  "babel": {
    "presets": [
      "es2015",
      "react"
    ],
    "plugins": [
      "transform-decorators-legacy"
    ]
  },
  "scripts": {
    "prebuild-css": "mkdirp dist/app/styles",
    "build-css": "sass app/styles/main.scss:dist/app/styles/main.css",
    "copy": "cp -R _libs dist/_libs && cp -R app/fonts dist/app/fonts && cp -R app/img dist/app/img",
    "build": "npm run build-css && npm run copy && .\\node_modules\\.bin\\webpack"
  },
  "dependencies": {
    "@angular/common": "^4.0.1",
    "@angular/compiler": "^4.0.1",
    "@angular/compiler-cli": "^4.0.1",
    "@angular/core": "^4.0.1",
    "@angular/forms": "^4.0.1",
    "@angular/http": "^4.0.1",
    "@angular/platform-browser": "^4.0.1",
    "@angular/platform-browser-dynamic": "^4.0.1",
    "@angular/router": "^4.0.1",
    "bootstrap": "^4.0.0-alpha.5",
    "core-js": "^2.4.1",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-router": "^3.0.2",
    "reflect-metadata": "^0.1.8",
    "rxjs": "5.0.1",
    "whatwg-fetch": "^1.0.0",
    "zone.js": "^0.7.4"
  },
  "devDependencies": {
    "autoprefixer": "^6.5.4",
    "babel-cli": "^6.18.0",
    "babel-loader": "^6.2.10",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-es2015-modules-systemjs": "^6.19.0",
    "babel-preset-es2015": "*",
    "babel-preset-es2016": "*",
    "babel-preset-react": "^6.22.0",
    "express": "^4.14.0",
    "html-loader": "^0.4.4",
    "mkdirp": "^0.5.1",
    "node-sass": "^3.13.0",
    "postcss": "^5.2.6",
    "systemjs": "0.19.41",
    "systemjs-plugin-text": "0.0.9",
    "uglify-js": "^2.7.4",
    "webpack": "^2.3.2",
    "yargs": "3.31.0"
  }
}
