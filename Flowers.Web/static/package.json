{
  "name": "flowers",
  "version": "1.0.0",
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins":[
      "transform-decorators-legacy"
    ]
  },
  "scripts": {
    "prebuild-css": "mkdirp dist/app/styles",
    "build-css": "sass app/styles/main.scss:dist/app/styles/main.css",
    "copy": "cp -R _libs dist/_libs && cp -R app/fonts dist/app/fonts && cp -R app/img dist/app/img",
    "build": "npm run build-css && npm run copy"
  },
  "dependencies": {
    "@angular/common": "2.4.1",
    "@angular/compiler": "2.4.1",
    "@angular/core": "2.4.1",
    "@angular/forms": "2.4.1",
    "@angular/http": "2.4.1",
    "@angular/platform-browser": "2.4.1",
    "@angular/platform-browser-dynamic": "2.4.1",
    "@angular/router": "3.4.1",
    "core-js": "^2.4.1",
    "reflect-metadata": "^0.1.8",
    "rxjs": "5.0.1",
    "whatwg-fetch": "^1.0.0",
    "zone.js": "^0.7.4",
    "bootstrap": "^4.0.0-alpha.5"
  },
  "devDependencies": {
    "autoprefixer": "^6.5.4",
    "babel-cli": "^6.18.0",
    "babel-loader": "^6.2.10",
    "babel-plugin-transform-es2015-modules-systemjs": "^6.19.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "*",
    "babel-preset-es2016": "*",
    "express": "^4.14.0",
    "html-loader": "^0.4.4",
    "mkdirp": "^0.5.1",
    "node-sass": "^3.13.0",
    "postcss": "^5.2.6",
    "systemjs": "0.19.41",
    "systemjs-plugin-text": "0.0.9",
    "uglify-js": "^2.7.4",
    "webpack": "^1.14.0",
    "yargs": "3.31.0"
  }
}
